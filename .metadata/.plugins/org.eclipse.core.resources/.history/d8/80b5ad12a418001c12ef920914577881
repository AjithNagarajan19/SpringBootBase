package com.basespring.springbootapp;
import org.springframework.web.bind.annotation.RequestMapping;
import org.springframework.web.bind.annotation.RestController;

@RestController
public class EmployeeController {

	
	 @RequestMapping("/")
	    public String home(){
		 String return_Value = "";
		 int count = 0;
		 if(count == 0){
			 return_Value = "True";
		 } else {
			 return_Value = "False";
		 }
		 
	        return return_Value;
	    }	
}
